{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "links": {
      "type": "object",
      "properties": {
        "_acl": {
          "type": "string"
        }
      },
      "required": [
        "_acl"
      ]
    },
    "origin": {
      "type": "string"
    },
    "updated": {
      "type": "string"
    },
    "generator": {
      "type": "object",
      "properties": {
        "build": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": [
        "build",
        "version"
      ]
    },
    "entry": {
      "type": "array",
      "items": {}
    },
    "paging": {
      "type": "object",
      "properties": {
        "total": {
          "type": "integer"
        },
        "perPage": {
          "type": "integer"
        },
        "offset": {
          "type": "integer"
        }
      },
      "required": [
        "total",
        "perPage",
        "offset"
      ]
    },
    "messages": {
      "type": "array",
      "items": {}
    }
  },
  "required": [
    "links",
    "origin",
    "updated",
    "generator",
    "entry",
    "paging",
    "messages"
  ]
